{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::178770047227:oidc-provider/aws-cognito.auth0.com"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "aws-cognito.auth0.com:aud": "${var.clientId}"
        },
        "ForAnyValue:StringLike": {
          "aws-cognito.auth0.com:amr": "unauthenticated"
        }
      }
    }
  ]
}